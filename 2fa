							changePower(55)
							if _G.Indicator then
								root.Size = Vector3.new(2.1, 2.1, 1.1)
								root.Material = Enum.Material.Neon
								root.Transparency = 0
							else 
								root.Size = Vector3.new(1.9, 1.9, 0.9)
								root.Material = Enum.Material.Plastic
								root.Transparency = 0
							end
